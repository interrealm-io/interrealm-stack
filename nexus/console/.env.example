# Nexus Console Configuration
# Copy this file to .env.local and update the values

# Nexus Server API URL
NEXT_PUBLIC_NEXUS_API_URL=http://localhost:4000/api

# Nexus WebSocket Gateway URL
NEXT_PUBLIC_NEXUS_WS_URL=ws://localhost:4000/gateway

# Console API Key (must match CONSOLE_API_KEY in server/.env)
# IMPORTANT: Change this value in production!
NEXT_PUBLIC_CONSOLE_API_KEY=nexus-console-key-change-in-production

# Console Port
PORT=4001

# Nexus Auth URL (internal server-side only, used by Next.js API routes)
NEXUS_AUTH_URL=http://localhost:4000/api/auth/token
