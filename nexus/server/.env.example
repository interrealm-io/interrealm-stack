# Nexus Database Configuration
DB_HOST=localhost
DB_PORT=5434
DB_NAME=neuxs
DB_USER=nexus_user
DB_PASSWORD=nexus_pass
DATABASE_URL=postgresql://nexus_user:nexus_pass@localhost:5434/neuxs

